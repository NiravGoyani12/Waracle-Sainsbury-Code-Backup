{
  "name": "fpp-test-automation",
  "version": "1.1.6",
  "description": "Automated BDD framework that uses webdriver.io, JavaScript and cucumber-js",
  "scripts": {
    "selenium-postinstall": "node_modules/.bin/selenium-standalone install",
    "selenium-start": "node_modules/.bin/selenium-standalone start",
    "test": "node test/config/bin/removeCucumberReports.js; node_modules/.bin/wdio ./test/config/main.conf.js",
    "bs-test": "node test/config/bin/removeBrowserStackReports.js; node_modules/.bin/wdio ./test/config/browserstack.conf.js",
    "bs-mobile-apple-test": "node test/config/bin/removeMobileBrowserStackReports.js; node_modules/.bin/wdio ./test/config/bs_mobile_apple.conf.js",
    "bs-mobile-samsung-test": "node test/config/bin/removeMobileBrowserStackReports.js; node_modules/.bin/wdio ./test/config/bs_mobile_samsung.conf.js",
    "lint": "eslint . --ext .js"
  },
  "dependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.2",
    "@babel/preset-env": "^7.14.2",
    "@babel/register": "^7.13.16",
    "@wdio/cli": "^7.3.1",
    "@wdio/cucumber-framework": "^7.4.6",
    "@wdio/local-runner": "^7.4.6",
    "@wdio/spec-reporter": "^7.4.3",
    "@wdio/sync": "^7.4.6",
    "axios": "^0.21.1",
    "base64topdf": "^1.1.8",
    "browserstack-local": "^1.4.8",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "chai-smoothie": "^0.3.2",
    "deepmerge": "^4.2.2",
    "dot-object": "^2.1.3",
    "dotenv": "^10.0.0",
    "enhanced-resolve": "^4.1.1",
    "faker": "^4.1.0",
    "graceful-fs": "^4.1.4",
    "jssha": "^3.2.0",
    "jwt-decode": "^2.2.0",
    "jwt-simple": "^0.5.6",
    "moment": "^2.29.1",
    "pdf-text-parser": "^0.2.1",
    "random-int": "^2.0.1",
    "soft-assert": "^0.2.0",
    "wdio-json-reporter": "^1.0.0",
    "webdriverio": "^7.13.1"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^7.2.1",
    "@wdio/browserstack-service": "^7.13.2",
    "@wdio/junit-reporter": "^5.16.11",
    "@wdio/sauce-service": "^5.0.0",
    "@wdio/selenium-standalone-service": "^7.8.0",
    "babel-cli": "^6.18.0",
    "chromedriver": "^92.0.0",
    "eslint": "^6.6.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-standard": "^4.0.1",
    "fs-extra": "^9.1.0",
    "geckodriver": "^2.0.4",
    "multiple-cucumber-html-reporter": "^1.18.0",
    "prettier": "^2.3.0",
    "selenium-standalone": "^7.1.0",
    "wdio-chromedriver-service": "^7.2.0",
    "wdio-cucumber-snippet-reporter": "0.0.5",
    "wdio-cucumberjs-json-reporter": "^1.3.0",
    "wdio-geckodriver-service": "^2.0.3",
    "wdio-intercept-service": "^4.1.7",
